## UTF-8.
defutf8 on

## C-] as the command key.
escape ^t

## Do not trash BackSpace, usually DEL.
bindkey -k kb
bindkey -d -k kb

## Do not trash Delete, usually ESC [ 3 ~.
bindkey -k kD
bindkey -d -k kD

# See /usr/share/terminfo/*/
term screen-256color
