Xft.dpi:       96
Xft.antialias: true
Xft.hinting:   slight
Xft.hintstyle: hintfull
Xft.rgba:      rgb
*font:         xft:DejaVu Sans Mono:pixelsize=12:antialias=true:hinting=true:autohint=true

Xcursor.theme: Vanilla-DMZ
! Xcursor.size: 48

! URxvt.
! Cursor disappears with compiz and Xft font
! <http://bbs.archlinux.org/viewtopic.php?pid=991221>.
! URxvt*cursorUnderline: true

*cursorUnderline: false
*cursorBlink: false

URxvt*geometry: 100x50
URxvt*scrollBar: true
urxvt.scrollstyle: plain
urxvt.scrollBar_right: true
! Black text on a white background.
urxvt.scrollColor: gray80
! ! White text on black background.
! urxvt.scrollColor: gray40

! urxvt.
! URxvt*font: -misc-fixed-medium-r-normal--18-120-100-100-c-90-iso10646-1,\
!             -misc-fixed-medium-r-normal--8-80-75-75-c-50-iso10646-1,\
!             -misc-fixed-medium-r-normal--8-80-75-75-c-50-iso10646-1,\
!             -misc-fixed-medium-r-normal--14-130-75-75-c-70-iso10646-1,\
!             -misc-fixed-medium-r-normal--13-120-75-75-c-80-iso10646-1

! "White" text on black background.
! URxvt*foreground: gray90
! URxvt*background: black

! Black text on a white background.
! URxvt*foreground: black
! URxvt*background: white

URxvt*saveLines: 10000

! urxvtperl
! See the urxvtperl manpage for details.
! You can find them in </usr/lib/urxvt/perl/>.
! <http://wiki.archlinux.org/index.php/Rxvt-unicode#Automatic_Script_Management>.
! selection,option-popup,selection-popup,readline,clipboard,matcher
! URxvt.perl-lib: /home/danil/.urxvt/ext/
URxvt*perl-ext-common: default,matcher,searchable-scrollback,clipboard
URxvt.searchable-scrollback: CM-F
URxvt.tabbed.tabbar-fg: 0
URxvt.tabbed.tabbar-bg: 15
Urxvt.tabbed.tab-fg:    0
URxvt.tabbed.tab-bg:    7
! URxvt.tabbed.hide: true
! URxvt.tabbed.newButton: false

! matcher.
URxvt*matcher.button: 3
!URxvt*colorUL: #86a2be
!URxvt*urlLauncher: x-www-browser
!URxvt*urlLauncher: /home/danil/bin/epiphany.sh
!URxvt*urlLauncher: chromium
URxvt*urlLauncher: google-chrome

urxvt*cursorColor: #aa0000

