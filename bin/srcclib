#!/usr/bin/env sh

# This file is part of Danil Kutkevich <danil@kutkevich.org> home.

install="$install clibs/clib-uninstall" # unistall bpkg package
install="$install clibs/clib-validate" # validate a package.json

if [ -n "$install" ] && [ "$install" != " " ]; then
    PREFIX=$HOME clib install "$install" # https://github.com/clibs/clib
fi
