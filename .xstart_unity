#! /bin/bash
# Running several applications from ~/.xinitrc
# <https://wiki.archlinux.org/index.php/ConsoleKit#Running_several_applications_from_.7E.2F.xinitrc>.

# Set the keyboard auto repeat parameters.
xset r rate 300 30

killall xbindkeys ; xbindkeys &
killall conky ; conky -c ~/.conky/conkyrc_calendar & conky -c ~/.conky/conkyrc_top & conky -c ~/.conky/conkyrc_clock_2 &
# killall parcellite ; parcellite &
killall glipper ; glipper &
killall stardict ; stardict &
killall wicd-gtk ; wicd-gtk --tray &
# killall empathy ; dbus-launch empathy &
killall gajim ; gajim &
