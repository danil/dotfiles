! UXTerm.
UXTerm*metaSendsEscape: true

! ! <http://tamentis.com/doc/xterm_resources/>.
! UXTerm*Translations: <Btn4Down>: larger-vt-font() \n\
! Ctrl <Btn5Down>: smaller-vt-font() \n\
! Shift Ctrl <Key>=: larger-vt-font() \n\
! Shift Ctrl <Key>-: smaller-vt-font()

UXTerm*faceSize:  14
UXTerm*faceSize1: 8
UXTerm*faceSize2: 9
UXTerm*faceSize3: 10
UXTerm*faceSize4: 12
UXTerm*faceSize5: 16
UXTerm*faceSize6: 24

UXTerm*font1: -misc-fixed-medium-r-normal--8-80-75-75-c-50-iso10646-1
UXTerm*font2: -misc-fixed-medium-r-normal--8-80-75-75-c-50-iso10646-1
UXTerm*font3: -misc-fixed-medium-r-normal--14-130-75-75-c-70-iso10646-1
UXTerm*font4: -misc-fixed-medium-r-normal--13-120-75-75-c-80-iso10646-1
UXTerm*font5: -misc-fixed-medium-r-normal--18-120-100-100-c-90-iso10646-1
UXTerm*font:  -misc-fixed-medium-r-normal--14-130-75-75-c-70-iso10646-1
UXTerm*font6: -misc-fixed-medium-r-normal--20-200-75-75-c-100-iso10646-1

! <http://tamentis.com/doc/xterm_resources>.
! UXTerm*Translations: <Key>F12: spawn-new-terminal()
