(global-set-key (kbd "C-c d l b") 'my-eval-buffer)
(global-set-key (kbd "C-c d l r") 'my-eval-region)
(defun my-eval-buffer ()
  (interactive)
  (eval-buffer)
  (message (format "Evaled %s" (buffer-name))))
(defun my-eval-region (start end)
  (interactive "r")
  (eval-region start end)
  (message "Region evaled"))
